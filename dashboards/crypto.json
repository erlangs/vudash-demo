{
  "name": "Cryptocoin Dashboard",
  "layout": {
    "columns": 6,
    "rows": 4
  },
  "datasources": {
    "ds-cryptocompare": { 
      "module": "plugins/cryptocompare-plugin",
      "schedule": 86400000,
      "options": {
        "symbols": ["BTC", "ETH"]
      }
    },
    "ds-cryptocompare-current-mtl": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "MTL"
      }
    },
    "ds-cryptocompare-current-sc": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "SC"
      }
    },
    "ds-cryptocompare-current-neo": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "NEO"
      }
    },
    "ds-cryptocompare-current-gnt": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "GNT"
      }
    },
    "ds-cryptocompare-current-strat": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "STRAT"
      }
    },
    "ds-cryptocompare-current-eth": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "ETH"
      }
    },
    "ds-cryptocompare-current-btc": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "BTC"
      }
    },
    "ds-cryptocompare-current-xrp": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "XRP"
      }
    },
    "ds-cryptocompare-current-xlm": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "XLM"
      }
    },
    "ds-cryptocompare-current-omg": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "OMG"
      }
    },
    "ds-cryptocompare-current-ltc": { 
      "module": "plugins/cryptocompare-current-plugin",
      "schedule": 30000,
      "options": {
        "symbol": "LTC"
      }
    }
  },
  "widgets": [
    {
      "position": {
        "x": 3,
        "y": 0,
        "w": 1,
        "h": 1
      },
      "datasource": "ds-cryptocompare-current-mtl",
      "options": {
        "description": "Metal",
        "format": "£%.2f"
      },
      "widget": "vudash-widget-statistic"
    },
    {
      "position": {
        "x": 4,
        "y": 0,
        "w": 1,
        "h": 1
      },
      "datasource": "ds-cryptocompare-current-sc",
      "options": {
        "schedule": 30000,
        "description": "Siacoin",
        "format": "£%.4f"
      },
      "widget": "vudash-widget-statistic"
    },
    {
      "position": {
        "x": 5,
        "y": 0,
        "w": 1,
        "h": 1
      },
      "datasource": "ds-cryptocompare-current-neo",
